{
  "name": "@ogfcommunity/variants-vue-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev-force": "vite --force",
    "start": "vite --force",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "gen-favicon": "rimraf public/favicon.ico && icon-gen --ico -i public/logo.svg -o public --ico-name favicon"
  },
  "lint-staged": {
    "src/**/*.+(ts|vue)": [
      "eslint"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "latest-3",
    "@ogfcommunity/variants-shared": "1.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/three": "^0.182.0",
    "@vueuse/core": "^10.2.1",
    "chessground": "^8.3.12",
    "npm-run-all2": "^6.1.2",
    "pinia": "^2.1.4",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.15.3",
    "three": "^0.182.0",
    "validator": "^13.15.23",
    "voronoijs": "^1.0.0",
    "vue": "^3.4.25",
    "vue-error-boundary": "^2.0.3",
    "vue-router": "^4.2.2",
    "vue3-chessboard": "^1.2.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^20.3.3",
    "@types/validator": "^13.15.10",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^10",
    "@vue/eslint-config-typescript": "^14",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-vue": "^10",
    "icon-gen": "^4.0.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.35",
    "postcss-nesting": "^12.0.4",
    "prettier": "^3",
    "rimraf": "^5.0.5",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8",
    "vite": "^4.5.2",
    "vitest": "^0.32.4",
    "vue-tsc": "^2.2.0"
  }
}
